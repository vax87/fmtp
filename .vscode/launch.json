            "env": {
                "GOOS": "windows",
                "GOARC": "386"
            }
        },
        {            
            "name": "fmtp_channel_x32",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/channel/fmtp_channel.go",
